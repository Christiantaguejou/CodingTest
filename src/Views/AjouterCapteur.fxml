<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="120.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AjouterCapteurController">
    <children>
        <GridPane layoutX="15.0" layoutY="29.0" prefHeight="57.0" prefWidth="270.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Nom" />
                <TextField fx:id="nomAjouter" GridPane.columnIndex="1" />
            </children>
        </GridPane>
        <ButtonBar layoutX="86.0" layoutY="75.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.rightAnchor="14.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#validerCapteur" text="Valider" />
                <Button mnemonicParsing="false" onAction="#annuler" text="Annuler" />
            </buttons>
        </ButtonBar>
    </children>

</AnchorPane>
